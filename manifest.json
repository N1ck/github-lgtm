{
  "name": "Github LGTM üëç",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "permissions": [
    "https://github.com/*"
  ],
  "content_scripts": [
    {
      "run_at": "document_end",
      "matches": [
        "https://github.com/**/pull/*"
      ],
      "js": [
        "scripts/vendor/jquery.min.js",
        "scripts/script.js"
      ]
    }
  ]
}
